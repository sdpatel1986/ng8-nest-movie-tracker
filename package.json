{
  "name": "movie-tracker",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "predev": "npm-run-all -l clean dev:env ",
    "dev": "npm-run-all -p -r run:services watch:**",
    "dev:server": "run-p -l clean run:services watch:server",
    "prebuild": "npm-run-all -l clean",
    "build": "if-env NODE_ENV=test && run-p -l build:** || run-s -l build:**",
    "build:movietracker": "ng build movietracker --prod",
    "build:server": "tsc -p ./apps/api/tsconfig.app.json",
    "watch:movietracker": "ng serve movietracker",
    "watch:server": "nodemon",
    "clean": "rimraf dist",
    "dev:env": "cp -n .env.template .env || true",
    "dev:env:force": "cp .env.template .env",
    "run:services": "docker-compose up && exit 0",
    "run:server": "node -r dotenv/config -r source-map-support/register dist/api/src/main.js",
    "stop:services": "docker-compose kill",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "yarn run typeorm migration:generate -n",
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "nx lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^6.2.4",
    "@nestjs/core": "^6.2.4",
    "@nestjs/platform-express": "^6.2.4",
    "reflect-metadata": "^0.1.12",
    "@angular/animations": "^8.0.0",
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/forms": "^8.0.0",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@angular/router": "^8.0.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.4.0",
    "zone.js": "^0.9.1",
    "express": "4.16.3",
    "@angular/cdk": "~8.1.0",
    "@angular/material": "^8.1.0",
    "@nestjs/typeorm": "^6.1.3",
    "@nrwl/angular": "8.2.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.9.1",
    "document-register-element": "1.13.1",
    "dotenv": "^8.0.0",
    "hammerjs": "^2.0.8",
    "if-env": "^1.0.4",
    "pg": "^7.11.0",
    "source-map-support": "^0.5.12",
    "tsconfig-paths": "^3.8.0",
    "typeorm": "^0.2.18"
  },
  "devDependencies": {
    "@nestjs/schematics": "^6.3.0",
    "@nestjs/testing": "^6.2.4",
    "@nrwl/nest": "8.2.0",
    "jest-preset-angular": "^7.1.1",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/language-service": "^8.0.0",
    "@angular-devkit/build-angular": "^0.800.0",
    "codelyzer": "~5.0.1",
    "@nrwl/node": "8.2.0",
    "@types/express": "4.16.0",
    "@nrwl/express": "8.2.0",
    "@angular/cli": "8.0.0",
    "@nrwl/cypress": "8.2.0",
    "@nrwl/jest": "8.2.0",
    "@nrwl/web": "8.2.0",
    "@nrwl/workspace": "8.2.0",
    "@types/jest": "24.0.9",
    "@types/node": "~8.9.4",
    "cypress": "^3.4.0",
    "husky": "^3.0.0",
    "jest": "24.1.0",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2",
    "ts-jest": "24.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.5"
  },
  "engines": {
    "node": "10.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,json,css,scss,md,html,component.html,graphql}": [
      "yarn run prettier --write",
      "git add"
    ],
    "*.ts": [
      "yarn run tslint --fix",
      "git add"
    ]
  }
}
